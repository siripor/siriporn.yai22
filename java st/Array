////Array วิธีที่ 2
let colors = ["แดง","ชมพู","เขียว","ขาว"];
colors[1] = "เหลือง";//สามารถระบุสีใหม่ด้วยตำแหน่ง
colors[2] = "ม่วง";
console.log(colors)

